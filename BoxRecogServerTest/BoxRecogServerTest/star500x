# a ini file for star and gg.
#
#define the transmission mode
TRANSMODE=0
#If no terminal, make the next line useful
PATH=/dev/ttyS0
#
SETAUX=\033!0;0;0;0Y\033[20h\033[12h
INITAUX=\033[/50h\033[/53h\033[/54l
CLOSEAUX=\033[/50l
WRITEAUX=\033[/51h
AUXENDWRITE=\033[/51l
DISABLEKEYBOARD=\033[2h
ENABLEKEYBOARD=\033[2l
