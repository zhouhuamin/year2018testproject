<?xml version="1.0" encoding="UTF-8" ?>   
<Config> 
    <GATHERGATE_TYPE>C</GATHERGATE_TYPE>
    <IS_VIRTUAL>1</IS_VIRTUAL>
    <CHANNEL_CONTROLER_PORT>9000</CHANNEL_CONTROLER_PORT>

    <SERVER_MONITOR_PORT>9600</SERVER_MONITOR_PORT>

    <CENTER_CONTROLER_IP>192.168.1.154</CENTER_CONTROLER_IP> 
    <CENTER_CONTROLER_PORT>20000</CENTER_CONTROLER_PORT> 
  
    <GATHER_AREA_ID>1234567890</GATHER_AREA_ID>
	 <GATHER_CHANNE_NO>1000000000</GATHER_CHANNE_NO>
    <GATHER_I_E_TYPE>I</GATHER_I_E_TYPE>

    <DEVICE>
      <DEVICEID IS_CAPTURE_DEVICE="1" TAG="IC">12010000</DEVICEID>
	   <DEVICEID IS_CAPTURE_DEVICE="1" TAG="SEAL">13000000</DEVICEID>
		<DEVICEID IS_CAPTURE_DEVICE="1" TAG="WEIGHT">15000000</DEVICEID>
      <DEVICEID IS_CAPTURE_DEVICE="1" TAG="CAR">11000000</DEVICEID>
	   <DEVICEID IS_CAPTURE_DEVICE="1" TAG="CONTA">16000000</DEVICEID>
      <DEVICEID IS_CAPTURE_DEVICE="0" TAG="DETECTOR">19000000</DEVICEID>
      <DEVICEID IS_CAPTURE_DEVICE="0" TAG="LCD">17000000</DEVICEID>
      <DEVICEID IS_CAPTURE_DEVICE="0" TAG="LEVER">18000000</DEVICEID>
	 </DEVICE>

	<CONTROL_SEQUENCE>

      <EVENT ASSOC="1" DESC="接收到车检器，车辆进入，读卡或者电子车牌">
      <RECV_EVENT>1900000001</RECV_EVENT>
     	<POST_EVENT TAG="IC">1201000000</POST_EVENT> 
      <POST_EVENT TAG="CAR">1100000000</POST_EVENT> 
      </EVENT>

      <EVENT ASSOC="0" DESC="读卡或者电子车牌,地磅工作">
      <RECV_EVENT>1201000001</RECV_EVENT>
      <RECV_EVENT>1100000002</RECV_EVENT>
     	<POST_EVENT TAG="WEIGHT">1500000000</POST_EVENT> 
      </EVENT>


	   <EVENT ASSOC="1" DESC="接收到地磅数据，箱号识别">
      <RECV_EVENT>1500000002</RECV_EVENT>
     	<POST_EVENT TAG="CONTA">1600000000</POST_EVENT> 
      </EVENT>


      <EVENT ASSOC="1" DESC="箱号识别完成，电子关锁">
      <RECV_EVENT>1600000002</RECV_EVENT>
     	<POST_EVENT TAG="SEAL">1300000000</POST_EVENT> 
      </EVENT>


      <EVENT ASSOC="1" DESC="电子关锁数据，打包发送">
      <RECV_EVENT>1300000001</RECV_EVENT>
     	<POST_EVENT TAG="CTRL">C000000001</POST_EVENT> 
      </EVENT>

      <EVENT ASSOC="1" DESC="平台返回控制指令">
      	<RECV_EVENT>C000000002</RECV_EVENT>
         <POST_EVENT TAG="IC">C000000003</POST_EVENT> 
      </EVENT>

		<EVENT ASSOC="1" DESC="解析控制指令完成">
      	<RECV_EVENT>C000000004</RECV_EVENT>
         <POST_EVENT TAG="LCD">2000000000</POST_EVENT> 
         <POST_EVENT TAG="LEVER">3000000000</POST_EVENT> 
         <POST_EVENT TAG="CTRL">C000000000</POST_EVENT> 
      </EVENT>

	</CONTROL_SEQUENCE>
   

   <EXCEPTION_SEQUENCE>
		<EVENT DESC="数据打包发送，等待平台返回">
      	<RECV_EVENT>C000000001</RECV_EVENT>
         <WAIT_EVENT>C000000002</WAIT_EVENT> 
         <MAX_WAIT>2000</MAX_WAIT>
         <POST_EVENT TAG="CTRL">C000000001</POST_EVENT> 
      </EVENT>
   </EXCEPTION_SEQUENCE>   
</Config>  
